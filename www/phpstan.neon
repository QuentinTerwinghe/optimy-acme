includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    level: 8
    paths:
        - app
        - config
        - database
        - routes
    excludePaths:
        - bootstrap
        - storage
        - vendor
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Class App\\Models\\(Category|Tag) uses generic trait Illuminate\\Database\\Eloquent\\Factories\\HasFactory but does not specify its types: TFactory#'
